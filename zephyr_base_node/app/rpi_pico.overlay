/ {
    chosen {
        zephyr,console = &uart0;
        zephyr,shell-uart = &uart0;
    };

    sensor_node_0: sensor_node_0 {
        compatible = "garden,sensor-node";
        status = "okay";
    };
};

&uart0 {
    status = "okay";
    current-speed = <115200>;
};

&uart1 {
    status = "okay";
    current-speed = <115200>;
};